<table>

<tr>

<th><h5>会員情報編集</h5></th>
<th><%= link_to "退会する", customers_withdraw_path(current_customer) %></th>

</tr>
<%= form_for(@customer, url: customers_update_path) do |f| %>
<tr>

<th>氏名</th>
<th><%= f.text_field :last_name, autofocus: true, placeholder:"苗字"%></th>
<th><%= f.text_field :first_name, autofocus: true, placeholder:"名前"%></th>

</tr>


<tr>

<th>カナ</th>
<th><%= f.text_field :last_name_kana, autofocus: true, placeholder:"苗字カナ"%></th>
<th><%= f.text_field :first_name_kana, autofocus: true, placeholder:"名前カナ"%></th>

</tr>



<tr>

<th>郵便番号</th>
<th><%= f.text_field :post_code, autofocus: true, placeholder:"郵便番号"%></th>

</tr>



<tr>

<th>住所</th>
<th><%= f.text_field :address, autofocus: true, placeholder:"住所"%></th>

</tr>



<tr>

<th>電話番号</th>
<th><%= f.text_field :phone_number, autofocus: true, placeholder:"電話番号"%></th>

</tr>



<tr>

<th>メールアドレス</th>
<th><%= f.text_field :last_name, autofocus: true, placeholder:"アドレス"%></th>

</tr>




<%= f.submit "Update User" %>



<% end %>



</table>